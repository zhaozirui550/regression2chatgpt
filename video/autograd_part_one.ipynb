    "def backward(root):\n",
    "    # 定义顶点的梯度等于1\n",
    "    root.grad = 1.0\n",
    "    ordered = _top_order(root)\n",
    "    for node in reversed(ordered):\n",
    "        for v in node.prevs:\n",
    "            v.grad += node.grad * node.grad_wrt[v]\n",
    "    return root"
   ]

    "# 拓扑排序\n",
    "def _top_order(root):\n",
    "    # 利用深度优先搜索\n",
    "    ordered, visited = [], set()\n",
    "    def _add_prevs(node):\n",
    "        if node not in visited:\n",
    "            visited.add(node)\n",
    "            for prev in node.prevs:\n",
    "                _add_prevs(prev)\n",
    "            ordered.append(node)\n",
    "    _add_prevs(root)\n",
    "    return ordered"
   ]
